<?xml version="1.0" encoding="utf-8"?>
<!--
    Backup Rules for Android 6-11 (API 23-30)
    
    This file specifies backup/restore behavior for older Android versions.
    We disable backup for security and privacy.
-->
<full-backup-content>
    <!-- Exclude all app data from backup -->
    <exclude domain="sharedpref" path="."/>
    <exclude domain="database" path="."/>
    <exclude domain="file" path="."/>
    <exclude domain="external" path="."/>
</full-backup-content>
